{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "properties": {
    "duration": {
      "type": "integer"
    },
    "locationId": {
      "type": "string"
    },
    "locationServiceGroupId": {
      "type": "string"
    },
    "organizationId": {
      "type": "string"
    },
    "resourceSelection": {
      "type": "string",
      "enum": ["ALLOWED", "REQUIRED", "DISABLED"]
    },
    "serviceId": {
      "type": "string"
    },
    "visibility": {
      "type": "object",
      "properties": {
        "monitor": {
          "type": "boolean"
        },
        "physicalKiosk": {
          "type": "boolean"
        },
        "webKiosk": {
          "type": "boolean"
        }
      }
    }
  }
}