{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "array",
  "items": [
    {
      "type": "object",
      "properties": {
        "description": {
          "anyOf": [
            {
              "type": "null"
            },
            {
              "type": "string"
            }
          ]
        },
        "id": {
          "type": "string"
        },
        "internalName": {
          "type": "string"
        },
        "nameTranslation": {
          "type": "string"
        },
        "phoneNumber": {
          "anyOf": [
            {
              "type": "null"
            },
            {
              "type": "string"
            }
          ]
        },
        "status": {
          "type": "string",
          "enum": [
            "ACTIVE",
            "INACTIVE"
          ]
        },
        "timezone": {
          "type": "string"
        }
      },
      "required": [
        "description",
        "id",
        "internalName",
        "nameTranslation",
        "phoneNumber",
        "status",
        "timezone"
      ]
    }
  ]
}