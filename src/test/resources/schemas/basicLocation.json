{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "properties": {
    "description": {
      "type": "string"
    },
    "id": {
      "type": "string"
    },
    "internalName": {
      "type": "string"
    },
    "displayName": {
      "type": "string"
    },
    "phoneNumber": {
      "type": "string"
    },
    "status": {
      "type": "string",
      "enum": [
        "ACTIVE",
        "INACTIVE"
      ]
    },
    "timezone": {
      "type": "string"
    }
  },
  "required": [
    "description",
    "id",
    "internalName",
    "status",
    "timezone"
  ]
}