{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "properties": {
    "id" : { "type" : "number" },
    "firstName" : { "type" : "string" },
    "lastName" : { "type" : "string" },
    "organizationId" : {
      "anyOf": [
        {
          "type" : "number"
        },
        {
          "type" : "null"
        }
      ]
    },
    "email" : { "type" : "string" }
  },
  "required": ["id", "firstName", "lastName", "organizationId", "email", "provider", "providerId", "type"]
}