{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "email": {
        "type": "string",
        "format": "email"
      },
      "status": {
        "type": "string",
        "enum": [
          "PENDING",
          "DRAFT"
        ]
      },
      "firstName": {
        "anyOf": [
          {
            "type": "string"
          },
          {
            "type": "null"
          }
        ]
      },
      "lastName": {
        "anyOf": [
          {
            "type": "string"
          },
          {
            "type": "null"
          }
        ]
      },
      "contactNumber": {
        "anyOf": [
          {
            "type": "string"
          },
          {
            "type": "null"
          }
        ]
      },
      "creationDate": {
        "anyOf": [
          {
            "type": "string"
          },
          {
            "type": "null"
          }
        ]
      },
      "expirationDate": {
        "type": "string"
      },
      "acceptanceDate": {
        "type": "null"
      },
      "inviterId": {
        "type": "string"
      }
    }
  }
}